{
  "name": "airbnb-catalyst",
  "version": "1.0.8",
  "description": "JavaScript Testing utilities for React / Flux",
  "main": "build",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src --out-dir build",
    "test": "mocha --compilers js:babel/register --recursive src/**/__tests__/*.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@git.musta.ch:airbnb/catalyst.git"
  },
  "keywords": [
    "javascript",
    "react",
    "flux",
    "testing",
    "tdd",
    "mocha"
  ],
  "author": "Leland Richardson <leland.richardson@airbnb.com>",
  "license": "MIT",
  "dependencies": {
    "alt": "^0.17.1",
    "cheerio": "^0.19.0",
    "sinon": "^1.15.4",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "mocha": "^2.2.5",
    "babel": "^5.8.21"
  },
  "peerDependencies": {
    "react": "0.13.x"
  },
  "optionalDependencies": {
    "jsdom": "^6.1.0",
    "mocha-jsdom": "^1.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.d.musta.ch"
  }
}
