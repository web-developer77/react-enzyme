{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "root": true,
  "rules": {
    "max-len": 0,
    "react/no-find-dom-node": 0,
    "react/no-multi-comp": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "react/no-render-return-value": 0, // react 15's `render` returns a value we use
  },
  "settings": {
    "react": {
      "version": "15.5.0",
    },
  },
}
