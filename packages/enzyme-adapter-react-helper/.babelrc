{
  "presets": ["airbnb"],
  "plugins": [
    ["transform-replace-object-assign", { "moduleSpecifier": "object.assign" }],
    ["add-module-exports"],
  ],
  "env": {
    "test": {
      "plugins": [
        ["transform-replace-object-assign", { "moduleSpecifier": "object.assign" }],
        "istanbul",
      ],
      "sourceMaps": "inline",
    },
  },
}
